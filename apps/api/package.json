{
  "name": "@texnika/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --ext .ts src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prisma:gen": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema prisma/schema.prisma",
    "search:reindex": "ts-node --transpile-only src/modules/search/scripts/reindex-listings.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "^10.3.3",
    "@prisma/client": "^5.9.1",
    "@texnika/shared": "workspace:*",
    "@opensearch-project/opensearch": "^2.7.1",
    "bullmq": "^4.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "minio": "^8.8.1",
    "nestjs-minio-client": "^2.5.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^8.17.1",
    "pino-http": "^9.0.0",
    "prisma": "^5.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.3.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/passport-jwt": "^3.0.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.15",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
